<div class="main login">
	<div class="title">
		<h2>LOG IN</h2>
	</div>
	<form action="login.do" method="POST">
		<table>
			<tr>
				<td>User name:</td>
				<td><input type="text" name="usr"></td>
			</tr>
			<tr>
				<td>Password:</td>
				<td><input type="password" name="pwd"></td>
			</tr>
			<tr>
				<td></td>
				<td><input type="submit" name="method" value="login"></td>
			</tr>
		</table>
	</form>
	<html:errors />
</div>

